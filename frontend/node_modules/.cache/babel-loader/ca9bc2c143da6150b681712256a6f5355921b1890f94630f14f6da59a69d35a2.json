} = await this.axios.get(\"http://localhost/api/bilibili/chart\", {\n        setTimeout: 10000\n      });\n      this.result = data[\"result\"];\n      this.index_number = data[\"result\"][data[\"result\"].length - 1][\"total_coin\"];\n      // console.log(this.follower)\n\n      let formattedData = this.result.map((e, i) => {\n        return {\n          x: e._id,\n          y: e.total_coin\n        };\n      });\n      let formattedData1 = this.result.map((e, i) => {\n        return {\n          x: e._id,\n          y: e.avg_coin\n        };\n      });\n\n      // update the series with axios data\n      this.series = [{\n        name: \"Total Coins\",\n        data: JSON.parse(JSON.stringify(formattedData))\n      }, {\n        name: \"Coins per video\",\n        data: JSON.parse(JSON.stringify(formattedData1))\n      }];\n      // console.log(this.series[0][data])\n      // console.log(typeof(this.series[0].values()))\n    },\n\n    update_data(timeline) {\n      this.selection = timeline;\n      this.axios.get(\"http://localhost/api/bilibili/chart\", {\n        setTimeout: 10000\n      }).then(res => {\n        this.data = res[\"data\"][\"result\"];\n        this.latest_date = this.data[this.data.length - 1][\"datetime\"];\n        this.one_month = this.data[this.data.length - 6][\"datetime\"];\n        this.three_months = this.data[this.data.length - 14][\"datetime\"];\n        this.six_months = this.data[this.data.length - 25][\"datetime\"];\n        this.one_year = this.data[this.data.length - 48][\"datetime\"];\n        this.latest_coin_count = this.data[this.data.length - 1][\"total_coin\"];\n        this.past_month_coin_count = this.data[this.data.length - 6][\"total_coin\"];\n\n        // console.log(this.latest_date)\n        // console.log(this.one_month)\n        // console.log(this.three_months)\n        // console.log(this.six_months)\n        // console.log(this.one_year)\n        // console.log(new Date(this.one_month).getTime())\n      }).catch(e