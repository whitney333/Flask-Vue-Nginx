{"ast":null,"code":"import VueApexCharts from \"vue-apexcharts\";\nexport default {\n  name: \"YoutubeTotalComment\",\n  components: {\n    apexchart: VueApexCharts\n  },\n  data() {\n    return {\n      series: [],\n      selection: 'one_month',\n      latest_date: '',\n      latest_comment_count: '',\n      past_month_comment_count: '',\n      index_number: '',\n      chartOptions: {\n        chart: {\n          id: 'youtube-total-video-comment',\n          height: '100%',\n          width: '100%',\n          type: 'line',\n          // group: 'tiktok',\n          toolbar: {\n            tools: {\n              download: true,\n              selection: true,\n              zoom: true,\n              zoomin: false,\n              zoomout: false,\n              pan: true,\n              reset: true\n            }\n          }\n        },\n        dataLabels: {\n          enabled: false\n        },\n        stroke: {\n          curve: 'smooth',\n          width: 3,\n          dashArray: [0, 2]\n        },\n        xaxis: {\n          // categories: [],\n          type: 'datetime',\n          labels: {\n            format: 'MM/dd',\n            rotate: -45,\n            trim: true,\n            style: {\n              fontSize: '12px',\n              fontWeight: 'bold',\n              fontFamily: 'Cairo'\n            }\n          },\n          tickAmount: 4,\n          tooltip: {\n            enabled: false\n          }\n        },\n        tooltip: {\n          theme: 'light',\n          custom: function ({\n            series,\n            seriesIndex,\n            dataPointIndex,\n            w\n          }) {\n            var data = w.globals.initialSeries[seriesIndex].data[dataPointIndex];\n            return '<div class=\"arrow_box\">' + new Date(data.x).toDateString() + \"<ul>\" + \"<span>\" + \"<li>\" +\n            // w.globals.labels[dataPointIndex] +\n            \"Total Comments: \" + series[0][dataPointIndex].toLocaleString() + \"</li>\" + \"</span>\" + \"<span>\" + \"<li>\" +\n            // w.globals.labels[dataPointIndex] +\n            \"Comments per video: \" + series[1][dataPointIndex].toLocaleString() + \"</li>\" + \"</span>\" + \"</ul>\" + \"</div>\";\n          }\n        },\n        legend: {\n          fontSize: '14px',\n          fontWeight: 'bold',\n          fontFamily: 'Cairo',\n          position: 'top',\n          horizontalAlign: 'left'\n        },\n        yaxis: [{\n          labels: {\n            style: {\n              fontSize: '12px',\n              fontWeight: 'bold',\n              fontFamily: 'Cairo'\n            },\n            formatter: function (value) {\n              if (String(value).length < 4) {\n                return Number(value).toLocaleString();\n              } else if (String(value).length < 7) {\n                return Number(value / 1000).toLocaleString() + 'K';\n              } else if (String(value).length < 10) {\n                return Number(value / 1000000).toLocaleString() + 'M';\n              } else {\n                return Number(value / 1000000000).toLocaleString() + 'B';\n              }\n            }\n          }\n        }, {\n          opposite: true,\n          labels: {\n            style: {\n              fontSize: '12px',\n              fontWeight: 'bold',\n              fontFamily: 'Cairo'\n            },\n            formatter: function (value) {\n              if (String(value).length < 4) {\n                return Number(value).toLocaleString();\n              } else if (String(value).length < 7) {\n                return Number(value / 1000).toLocaleString() + 'K';\n              } else if (String(value).length < 10) {\n                return Number(value / 1000000).toLocaleString() + 'M';\n              } else {\n                return Number(value / 1000000000).toLocaleString() + 'B';\n              }\n            }\n          }\n        }],\n        colors: ['#ff0000', '#ff0000'],\n        grid: {\n          show: false\n        }\n      }\n    };\n  },\n  methods: {\n    async get_youtube_video_comment() {\n      const {\n        data\n      } = await this.axios.get(\"http://localhost/api/youtube/stats/video\", {\n        setTimeout: 10000\n      });\n      this.youtube_videos = data[\"result\"];\n      this.index_number = data[\"result\"][data[\"result\"].length - 1][\"total_comment\"];\n      // console.log(this.youtube_videos)\n\n      let formattedData = this.youtube_videos.map((e, i) => {\n        return {\n          x: e.datetime,\n          y: e.total_comment\n        };\n      });\n      let formattedData1 = this.youtube_videos.map((e, i) => {\n        return {\n          x: e.datetime,\n          y: e.avg_comment\n        };\n      });\n      // update the series with axios data\n      this.series = [{\n        name: \"Total Comments\",\n        data: formattedData\n      }, {\n        name: \"Comments per video\",\n        data: formattedData1\n      }];\n    },\n    update_data(timeline) {\n      this.selection = timeline;\n      this.axios.get(\"http://localhost/api/youtube/stats/video\", {\n        setTimeout: 10000\n      }).then(res => {\n        this.data = res[\"data\"][\"result\"];\n        this.latest_date = this.data[this.data.length - 1][\"datetime\"];\n        this.one_month = this.data[this.data.length - 30][\"datetime\"];\n        this.three_months = this.data[this.data.length - 90][\"datetime\"];\n        this.six_months = this.data[this.data.length - 180][\"datetime\"];\n        // this.one_year = this.data[this.data.length - 48][\"datetime\"]\n\n        this.latest_comment_count = this.data[this.data.length - 1][\"total_comment\"];\n        this.past_month_comment_count = this.data[this.data.length - 30][\"total_comment\"];\n\n        // console.log(this.latest_date)\n        // console.log(this.one_month)\n        // console.log(this.three_months)\n        // console.log(this.six_months)\n        // console.log(this.one_year)\n        // console.log(new Date(this.one_month).getTime())\n      }).catch(err => {\n        console.log(err);\n      });\n      switch (timeline) {\n        case 'one_month':\n          this.$refs.chart.zoomX(new Date(this.one_month).getTime(), new Date(this.latest_date).getTime());\n          break;\n        case 'three_months':\n          this.$refs.chart.zoomX(new Date(this.three_months).getTime(), new Date(this.latest_date).getTime());\n          break;\n        case 'six_months':\n          this.$refs.chart.zoomX(new Date(this.six_months).getTime(), new Date(this.latest_date).getTime());\n          break;\n        case 'one_year':\n          this.$refs.chart.zoomX(new Date(this.one_year).getTime(), new Date(this.latest_date).getTime());\n      }\n    }\n  },\n  created() {\n    this.get_youtube_video_comment();\n    this.update_data();\n  },\n  filters: {\n    formatNumber: function (value) {\n      if (String(value).length < 4) {\n        return Number(value).toLocaleString();\n      } else if (String(value).length < 7) {\n        return Number(value / 1000).toLocaleString() + 'K';\n      } else if (String(value).length < 10) {\n        return Number(value / 1000000).toLocaleString() + 'M';\n      } else {\n        return Number(value / 1000000000).toLocaleString() + 'B';\n      }\n    }\n  }\n};","map":{"version":3,"names":["VueApexCharts","name","components","apexchart","data","series","selection","latest_date","latest_comment_count","past_month_comment_count","index_number","chartOptions","chart","id","height","width","type","toolbar","tools","download","zoom","zoomin","zoomout","pan","reset","dataLabels","enabled","stroke","curve","dashArray","xaxis","labels","format","rotate","trim","style","fontSize","fontWeight","fontFamily","tickAmount","tooltip","theme","custom","seriesIndex","dataPointIndex","w","globals","initialSeries","Date","x","toDateString","toLocaleString","legend","position","horizontalAlign","yaxis","formatter","value","String","length","Number","opposite","colors","grid","show","methods","get_youtube_video_comment","axios","get","setTimeout","youtube_videos","formattedData","map","e","i","datetime","y","total_comment","formattedData1","avg_comment","update_data","timeline","then","res","one_month","three_months","six_months","catch","err","console","log","$refs","zoomX","getTime","one_year","created","filters","formatNumber"],"sources":["src/components/YoutubeTotalComment.vue"],"sourcesContent":["<template>\r\n  <div id=\"youtube-total-video-comment-chart\">\r\n    <div class=\"toolbar\">\r\n      <v-row justify=\"end\">\r\n        <v-col cols=\"12\">\r\n          <div class=\"index_number\">\r\n            {{ Number(this.index_number) | formatNumber }}\r\n          </div>\r\n          <v-btn\r\n              outlined\r\n              x-small\r\n              color=\"blue-grey lighten-1\"\r\n              dark\r\n              rounded\r\n              elevation=\"1\"\r\n              class=\"mr-3\"\r\n              @click=\"update_data('one_month')\" :class=\"{active: selection==='one_month'}\"\r\n          >\r\n            1M\r\n          </v-btn>\r\n          <v-btn\r\n              outlined\r\n              x-small\r\n              color=\"blue-grey lighten-1\"\r\n              dark\r\n              rounded\r\n              elevation=\"1\"\r\n              class=\"mr-3\"\r\n              @click=\"update_data('three_months')\" :class=\"{active: selection==='three_months'}\"\r\n          >\r\n            3M\r\n          </v-btn>\r\n          <v-btn\r\n              x-small\r\n              outlined\r\n              color=\"blue-grey lighten-1\"\r\n              dark\r\n              rounded\r\n              elevation=\"1\"\r\n              class=\"mr-3\"\r\n              @click=\"update_data('six_months')\" :class=\"{active: selection==='six_months'}\"\r\n          >\r\n            6M\r\n          </v-btn>\r\n          <v-btn\r\n              x-small\r\n              outlined\r\n              color=\"blue-grey lighten-1\"\r\n              dark\r\n              rounded\r\n              elevation=\"1\"\r\n              class=\"mr-3\"\r\n              @click=\"update_data('one_year')\" :class=\"{active: selection==='one_year'}\"\r\n          >\r\n            1Y\r\n          </v-btn>\r\n        </v-col>\r\n        <v-col cols=\"12\">\r\n          <div class=\"chart-latest-update\">\r\n            <span>{{ $t('Last updated') }}\r\n              <span>{{ this.latest_date }}</span>\r\n            </span>\r\n          </div>\r\n          <div class=\"month_change_percentage\">\r\n            <div class=\"stat\">\r\n              <span class=\"up\"\r\n                    v-if=\"(((this.latest_comment_count - this.past_month_comment_count)/this.past_month_comment_count)*100)>0\">\r\n                    +{{Number((((this.latest_comment_count - this.past_month_comment_count) / this.past_month_comment_count) * 100).toFixed(2)).toLocaleString() }}%\r\n              </span>\r\n              <span class=\"down\"\r\n                    v-if=\"(((this.latest_comment_count - this.past_month_comment_count)/this.past_month_comment_count)*100)<0\">\r\n                    {{Number((((this.latest_comment_count - this.past_month_comment_count) / this.past_month_comment_count) * 100).toFixed(2)).toLocaleString() }}%\r\n              </span>\r\n              <span class=\"same\"\r\n                    v-if=\"(((this.latest_comment_count - this.past_month_comment_count)/this.past_month_comment_count)*100)===0\">\r\n                    {{Number((((this.latest_comment_count - this.past_month_comment_count) / this.past_month_comment_count) * 100).toFixed(2)).toLocaleString() }}%\r\n              </span>\r\n              <span class=\"update_date\">{{ $t('Past Month') }}</span>\r\n            </div>\r\n          </div>\r\n        </v-col>\r\n      </v-row>\r\n    </div>\r\n    <apexchart\r\n        ref=\"chart\"\r\n        :options=\"chartOptions\"\r\n        :series=\"series\"\r\n        width=\"375\"\r\n        height=\"375\"\r\n    >\r\n    </apexchart>\r\n  </div>\r\n</template>\r\n<script>\r\nimport VueApexCharts from \"vue-apexcharts\";\r\nexport default {\r\n  name: \"YoutubeTotalComment\",\r\n  components: {\r\n    apexchart: VueApexCharts,\r\n  },\r\n  data() {\r\n    return {\r\n      series: [],\r\n      selection: 'one_month',\r\n      latest_date: '',\r\n      latest_comment_count: '',\r\n      past_month_comment_count: '',\r\n      index_number: '',\r\n      chartOptions: {\r\n        chart: {\r\n          id: 'youtube-total-video-comment',\r\n          height: '100%',\r\n          width: '100%',\r\n          type: 'line',\r\n          // group: 'tiktok',\r\n          toolbar: {\r\n            tools: {\r\n              download: true,\r\n              selection: true,\r\n              zoom: true,\r\n              zoomin: false,\r\n              zoomout: false,\r\n              pan: true,\r\n              reset: true\r\n            }\r\n          }\r\n        },\r\n        dataLabels: {\r\n          enabled: false,\r\n        },\r\n        stroke: {\r\n          curve: 'smooth',\r\n          width: 3,\r\n          dashArray: [0, 2]\r\n        },\r\n        xaxis: {\r\n          // categories: [],\r\n          type: 'datetime',\r\n          labels: {\r\n            format: 'MM/dd',\r\n            rotate: -45,\r\n            trim: true,\r\n            style: {\r\n              fontSize: '12px',\r\n              fontWeight: 'bold',\r\n              fontFamily: 'Cairo',\r\n            }\r\n          },\r\n          tickAmount: 4,\r\n          tooltip: {\r\n            enabled: false\r\n          }\r\n        },\r\n        tooltip: {\r\n          theme: 'light',\r\n          custom: function ({series, seriesIndex, dataPointIndex, w}) {\r\n            var data = w.globals.initialSeries[seriesIndex].data[dataPointIndex];\r\n            return (\r\n                '<div class=\"arrow_box\">' +\r\n                new Date(data.x).toDateString() +\r\n                \"<ul>\" +\r\n                \"<span>\" +\r\n                \"<li>\" +\r\n                // w.globals.labels[dataPointIndex] +\r\n                \"Total Comments: \" +\r\n                (series[0][dataPointIndex]).toLocaleString() +\r\n                \"</li>\" +\r\n                \"</span>\" +\r\n                \"<span>\" +\r\n                \"<li>\" +\r\n                // w.globals.labels[dataPointIndex] +\r\n                \"Comments per video: \" +\r\n                (series[1][dataPointIndex]).toLocaleString() +\r\n                \"</li>\" +\r\n                \"</span>\" +\r\n                \"</ul>\" +\r\n                \"</div>\"\r\n            );\r\n          }\r\n        },\r\n        legend: {\r\n          fontSize: '14px',\r\n          fontWeight: 'bold',\r\n          fontFamily: 'Cairo',\r\n          position: 'top',\r\n          horizontalAlign: 'left'\r\n        },\r\n        yaxis: [\r\n          {\r\n            labels: {\r\n              style: {\r\n                fontSize: '12px',\r\n                fontWeight: 'bold',\r\n                fontFamily: 'Cairo',\r\n              },\r\n              formatter: function (value) {\r\n                if (String(value).length < 4) {\r\n                  return Number(value).toLocaleString();\r\n                } else if (String(value).length < 7) {\r\n                  return Number(value / 1000).toLocaleString() + 'K';\r\n                } else if (String(value).length < 10) {\r\n                  return Number(value / 1000000).toLocaleString() + 'M';\r\n                } else {\r\n                  return Number(value / 1000000000).toLocaleString() + 'B';\r\n                }\r\n              },\r\n            }\r\n          },\r\n          {\r\n            opposite: true,\r\n            labels: {\r\n              style: {\r\n                fontSize: '12px',\r\n                fontWeight: 'bold',\r\n                fontFamily: 'Cairo',\r\n              },\r\n              formatter: function (value) {\r\n                if (String(value).length < 4) {\r\n                  return Number(value).toLocaleString();\r\n                } else if (String(value).length < 7) {\r\n                  return Number(value / 1000).toLocaleString() + 'K';\r\n                } else if (String(value).length < 10) {\r\n                  return Number(value / 1000000).toLocaleString() + 'M';\r\n                } else {\r\n                  return Number(value / 1000000000).toLocaleString() + 'B';\r\n                }\r\n              },\r\n            }\r\n          },\r\n        ],\r\n        colors: ['#ff0000', '#ff0000'],\r\n        grid: {\r\n          show: false\r\n        }\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    async get_youtube_video_comment() {\r\n    const {data} = await this.axios.get(\"http://localhost/api/youtube/stats/video\", {setTimeout: 10000})\r\n    this.youtube_videos = data[\"result\"]\r\n    this.index_number = data[\"result\"][data[\"result\"].length - 1][\"total_comment\"]\r\n    // console.log(this.youtube_videos)\r\n\r\n      let formattedData = this.youtube_videos.map((e, i) => {\r\n        return {\r\n          x: e.datetime,\r\n          y: e.total_comment,\r\n        };\r\n      });\r\n      let formattedData1 = this.youtube_videos.map((e, i) => {\r\n        return {\r\n          x: e.datetime,\r\n          y: e.avg_comment,\r\n        };\r\n      });\r\n      // update the series with axios data\r\n      this.series = [\r\n        {\r\n          name: \"Total Comments\",\r\n          data: formattedData,\r\n        },\r\n        {\r\n          name: \"Comments per video\",\r\n          data: formattedData1,\r\n        }\r\n      ]\r\n    },\r\n    update_data(timeline) {\r\n      this.selection = timeline\r\n\r\n      this.axios.get(\"http://localhost/api/youtube/stats/video\", {setTimeout: 10000})\r\n          .then(res => {\r\n            this.data = res[\"data\"][\"result\"]\r\n            this.latest_date = this.data[this.data.length - 1][\"datetime\"]\r